        var tabs, tabId;
 
        //Get parameter
        var parameters = location.search;
        var index = parameters.indexOf('tabIndex=');
        if (index != -1){
          
          var tabIndex = parameters.charAt(index+9);
          if(tabsdiv.childNodes.length == 1)
